/* pages/score/score.wxss */
page{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-size: cover;
  background-image: url("data:image/jpg;base64,");
}
.user_video{
  width: 100vw;
  height: 40vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #000000;
}
.user_video video{
  width: 100vw;
}

swiper{
  width: 100vw;
  height: 40vh;
}
swiper-item{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.user_photo{
  position: relative;
  width: 100%;
}

.score_container{
  position: relative;
  margin-top: 28rpx;
  width: 681rpx;
  height: 444rpx;
  background-color: #313131;
  border-radius: 36rpx; 
  border-color: #FFFFFF;
  box-shadow: 0rpx 2rpx 6rpx 0rpx rgba(0, 0, 0, 0.4);
  display: flex;
  align-items: center;
  justify-content: center;
}
.score_container .share{
  position: absolute;
  top: 2vh;
  left: 80.6vw;
  width: 5vw;
}
.share image{
  width: 100%;
}
.share button{
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0.01;
}
.score_container .title{
  position: absolute;
  top: 4.1vh;
  color: #E7E7E7;
  font-size: 32.6rpx;
  font-family: PingFangSC-regular;
}
.score_container .score{
  color: #FFD200;
  font-size: 130.4rpx;
  font-family: PingFangSC-bold;
  font-weight: 600;
}

.button_container{
  width: 100vw;
  position: relative;
  margin-top: 41.7rpx;
}
.again_button{
  position: absolute;
  top : 0;
  right: 67rpx;
  width: 224rpx;
  height: 92.4rpx;
  background-color: #313131;
  border-radius: 36rpx; 
  display: flex;
  align-items: center;
  justify-content: center;
  border-color: #FFFFFF;
  box-shadow: 0rpx 2rpx 6rpx 0rpx rgba(0, 0, 0, 0.4);
}
.log_button{
  position: absolute;
  top: 0;
  left: 67rpx;
  width: 224rpx;
  height: 92.4rpx;
  background-color: #313131;
  border-radius: 36rpx; 
  display: flex;
  align-items: center;
  justify-content: center;
  border-color: #FFFFFF;
  box-shadow: 0rpx 2rpx 6rpx 0rpx rgba(0, 0, 0, 0.4);
}

.button_text{
  font-size: 25.5rpx;
  color: #E7E7E7;
}
.button_hover{
  transform: scale(0.9,0.9);/* 缩放 */
}
